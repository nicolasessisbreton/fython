linecode is packagebol . nothing to do ; already handled by importec in previous linecod

linefeedx newlinex . blank line, nothing to do ; can happen after an interpolation

linefeedx ibol . iruc

linefeedx element newlinex . unaryR
	element
		break . breakruc
		continue .continueruc
		pass . passruc
		return . returnruc
		stop . stopruc
		? . elementruc

linefeedx element0 element1 element* terminal. genericR
	element0
		alloc . allocruc
		class . classpec
		critical . criticalruc
		dealloc . deallocruc
		def . defR
			terminal
				ibol . lexiruc
				childcod . routpec
		elif . elifruc
		else . elseruc
		elwhere . elwhereruc
		error . errorruc
		if . ifruc
		import . importpec
		inline . inlineruc
		interface . interfacepec
		for . forruc
		omp . ompruc
		print . printruc
		private . privateruc
		public . publicruc
		read . readruc
		sync . syncruc
		where . whereruc
		while . whileruc
		xip . xipruc
		? . varpec	